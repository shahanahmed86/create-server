{
	"name": "my_project",
	"description": "Boiler plate for nodejs application with graphql & express",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"prepare": "echo 'installing husky....' && husky install",
		"dev": "cross-env NODE_ENV='development' nodemon -r esm .",
		"start": "cross-env NODE_ENV='production' node -r esm .",
		"db:format": "prisma format",
		"predb:deploy": "npm run db:format",
		"db:deploy": "prisma migrate dev --name init",
		"postdb:deploy": "npm run db:seed",
		"db:seed": "prisma db seed",
		"db:reset": "prisma migrate reset",
		"db:generate": "prisma generate",
		"lint": "eslint '**/*.js'",
		"prettify": "prettier --no-editorconfig -l --write '**/*.{js,json}'",
		"exec-tests": "mocha -r esm src/tests/**/*.spec.js --exit",
		"test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit",
		"posttest": "docker-compose -f docker-compose.yml -f docker-compose.test.yml down -v"
	},
	"lint-staged": {
		"**/*.{js,json}": [
			"npm run lint",
			"npm run prettify"
		]
	},
	"prisma": {
		"seed": "node -r esm prisma/seed.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/shahanahmed86/my_project.git"
	},
	"keywords": [
		"boiler-plate",
		"nodejs",
		"apollo server express",
		"apollo server"
	],
	"author": "Shahan",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/shahanahmed86/my_project.git/issues"
	},
	"homepage": "https://github.com/shahanahmed86/my_project.git#readme",
	"dependencies": {
		"@graphql-tools/schema": "^8.3.1",
		"@graphql-tools/utils": "^8.5.5",
		"@prisma/client": "^3.6.0",
		"apollo-server-core": "^3.5.0",
		"apollo-server-express": "^3.5.0",
		"arg": "^5.0.1",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
		"ejs": "^3.1.6",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^5.2.0",
		"esm": "^3.2.25",
		"express": "^4.17.1",
		"express-fileupload": "^1.2.1",
		"graphql": "^15.8.0",
		"husky": "^7.0.4",
		"inquirer": "^8.2.0",
		"ioredis": "^4.28.2",
		"joi": "^17.5.0",
		"jsonwebtoken": "^8.5.1",
		"lint-staged": "^12.1.2",
		"moment": "^2.29.1",
		"morgan": "^1.10.0",
		"prettier": "^2.5.1",
		"prisma": "^3.6.0",
		"swagger-jsdoc": "^6.1.0",
		"swagger-ui-express": "^4.3.0"
	},
	"devDependencies": {
		"chai": "^4.3.4",
		"chai-http": "^4.3.0",
		"mocha": "^8.4.0",
		"nodemon": "^2.0.15"
	}
}
